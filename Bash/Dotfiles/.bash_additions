#!/bin/bash

export PATH="$HOME/bin:$PATH"

alias python="python3"

if [ -f "$HOME/.bashrc" ]; then
	alias r="source $HOME/.bashrc"
elif [ -f "$HOME/.bash_profile" ]; then
	alias r="source $HOME/.bash_profile"
fi

if [[ $(uname) == Darwin ]]; then
    alias ls="ls -Gp"
    alias la="ls -GpA"
    alias ll="ls -Gpoh"
    alias lal="ls -GpAoh"
elif [[ $(uname) == Linux ]]; then
    alias ls="ls -p --color"
    alias la="ls -pA --color"
    alias ll="ls -poh --color"
    alias lal="ls -pAoh --color"
fi

aws_ssh_cmd="ssh -i ~/.ssh/Main.pem ubuntu@ec2-34-210-71-203.us-west-2.compute.amazonaws.com"
uuid="$(cat ~/.instance_uuid | awk '{print $NF}')"
if [[ $uuid == "9344462f-6d26-4f5c-bf78-0712eb2fc56b" ]]; then
    echo "macbook air"
    alias ssh-to-aws="$aws_ssh_cmd"
elif [[ $uuid == "b967243a-ee97-4320-8b0e-3101ffd28929" ]]; then
    echo "ubuntu virtualbox"
    # TODO: delete MainEnv
    alias ssh-to-aws="$aws_ssh_cmd"
elif [[ $uuid == "0b5f4d6c-024e-411f-8dc8-87dbf6eb5eaa" ]]; then
    echo "aws1 main"
elif [[ $uuid == "88af29fe-fda2-4bcc-a65a-cfc61f9ef0b4" ]]; then
    echo "raspberry pi"
    alias ssh-to-aws="$aws_ssh_cmd"
elif [[ $uuid == "132b41dc-a118-4188-8bbf-1728c392d723" ]]; then
    echo "pythonanywhere"
    source ~/.virtualenvs/MainEnv/bin/activate
fi

mkcd() {
	if [[ $# != 1 ]]; then
		echo "Usage: mkcd DIR"
	else
		mkdir -p -- "$1" && cd -P -- "$1"
	fi
}

clera() {
    a="\x20"; b="\x23"; c="\x5f"; d="\x2f"; e="\x7c"; f="\x5c"; g="\x3a"
    h="\x38"; i="\x24"; j="\xe2"; k="\x96"; l="\x88"; m="\x95"; n="\x90"
    choices=("$a$b$b$b$b$b$a$a$b$a$a$a$a$a$a$a$b$b$b$b$b$b$b$a$b$b$b$b$b$b$a$a$a$a$a$b$a$a$a$a\xa$b$a$a$a$a$a$b$a$b$a$a$a$a$a$a$a$b$a$a$a$a$a$a$a$b$a$a$a$a$a$b$a$a$a$b$a$b$a$a$a\xa$b$a$a$a$a$a$a$a$b$a$a$a$a$a$a$a$b$a$a$a$a$a$a$a$b$a$a$a$a$a$b$a$a$b$a$a$a$b$a$a\xa$b$a$a$a$a$a$a$a$b$a$a$a$a$a$a$a$b$b$b$b$b$a$a$a$b$b$b$b$b$b$a$a$b$a$a$a$a$a$b$a\xa$b$a$a$a$a$a$a$a$b$a$a$a$a$a$a$a$b$a$a$a$a$a$a$a$b$a$a$a$b$a$a$a$b$b$b$b$b$b$b$a\xa$b$a$a$a$a$a$b$a$b$a$a$a$a$a$a$a$b$a$a$a$a$a$a$a$b$a$a$a$a$b$a$a$b$a$a$a$a$a$b$a\xa$a$b$b$b$b$b$a$a$b$b$b$b$b$b$b$a$b$b$b$b$b$b$b$a$b$a$a$a$a$a$b$a$b$a$a$a$a$a$b"
